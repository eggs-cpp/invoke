#if BENCHMARK == 2
#include <functional>
#elif BENCHMARK == 3
#include <eggs/invoke.hpp>
#endif

template <int I>
struct mem
{
    int obj;
};

template <int I>
static void benchmark()
{
#if BENCHMARK == 1
    (void)(mem<I>{}.*(&mem<I>::obj));
#elif BENCHMARK == 2
    std::invoke(&mem<I>::obj, mem<I>{});
#elif BENCHMARK == 3
    eggs::invoke(&mem<I>::obj, mem<I>{});
#endif
}

@BENCHMARK_INSTANTIATIONS@
