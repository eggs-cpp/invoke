#if BENCHMARK == 2
#include <functional>
#elif BENCHMARK == 3
#include <eggs/invoke.hpp>
#endif

template <int I>
struct mem
{
    int fun(...) { return I; };
};

template <int I>
static void benchmark()
{
#if BENCHMARK == 1
    (mem<I>{}.*(&mem<I>::fun))(0, 1, 2);
#elif BENCHMARK == 2
    std::invoke(&mem<I>::fun, mem<I>{}, 0, 1, 2);
#elif BENCHMARK == 3
    eggs::invoke(&mem<I>::fun, mem<I>{}, 0, 1, 2);
#endif
}

@BENCHMARK_INSTANTIATIONS@
